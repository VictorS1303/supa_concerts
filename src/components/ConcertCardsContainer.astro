---
import Card from "./Card.astro"
const { class: className, id: id, concertCards = [] } = Astro.props
---

<div class="flex flex-col justify-center items-center mt-20">
  {
    concertCards.length > 0
      ? concertCards.map((concertCard) => (
          <a href="#">
            <Card
              isConcertCard={true}
              src={concertCard.src}
              alt={concertCard.alt}
              artistNameText={concertCard.artistNameText}
              genreText={concertCard.genreText}
            />
          </a>
        ))
      : "Ingen koncerter"
  }
</div>
